---
import Layout from "../layouts/Layout.astro";
import Nav from "../components/Nav.astro";
import Footer from "../components/Footer.astro";

---

<Layout title="hackathon ia ">

    <main>
       <div class="min-h-10 py-3 w-full bg-cover bg-no-repeat" style="background-image: url('/fond-senia.jpg');">
         <Nav lien1="/programmes" lien2="/participation" lien3="/ressources" lien4="/galerie" lien5="/contact" lien="../" lien6="/hackathonIA"/>
       </div>
       <div class="w-[80%] mx-auto my-24">
         <img src="/hackathon.png" alt="max-w-full">

         <div class="w-full flex mt-4">
            <button class=" mx-auto  uppercase bg-gradient-to-r from-[#273e95]  to-[#227aac] to-90% hover:from-[#227aac]  hover:to-[#227aac] hover:to-90% font-[500] text-white p-2 rounded-tr-full rounded-l-full">Téléchargez le dossier documentaire</button>
         </div>
       </div>

       <div>
         <Footer/>
       </div>
    </main>
</Layout>